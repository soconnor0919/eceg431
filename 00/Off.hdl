/**
 * Off gate:
 * out = false (off)
 */

CHIP Off {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    // Nand(a=true, b=false, out=out);  // old
    Nand(a=true, b=true, out=out);
}
