/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=tmp1);
    Not(in=b, out=tmp2);
    And(a=a, b=tmp2, out=tmp3);
    And(a=tmp1, b=b, out=tmp4);
    Or(a=tmp3, b=tmp4, out=out);
}
